{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "pragma": "React.createElement",
        "pragmaFrag": "React.Fragment"
      }
    },
    "target": "es2017",
    "loose": false,
    "externalHelpers": false
  },
  "minify": {
    "compress": {
      "unused": true
    },
    "mangle": true
  }
}
