{
  "name": "dads-first-step",
  "version": "0.2.0",
  "private": true,
  "description": "A comprehensive baby milestone tracking application for fathers",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "format": "prettier --write \"src/**/*.{ts,tsx,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,md,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "clean": "rimraf .next out build",
    "reset": "npm run clean && npm cache clean --force && npm install"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase": "^11.2.0",
    "framer-motion": "^11.18.0",
    "next": "^15.1.4",
    "next-themes": "^0.4.4",
    "react": "^18.2.0",
    "react-dom": "^19.0.0",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.4.0",
    "sonner": "^1.7.1",
    "tailwind-merge": "^2.6.0",
    "zustand": "latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@next/eslint-plugin-next": "^15.1.4",
    "@types/node": "^20.17.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-next": "^15.1.5",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run typecheck",
      "pre-push": "npm test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
